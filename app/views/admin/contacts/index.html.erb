<div class="container">
  <div class="row">
    <div class="col-8 mx-auto">
      <div class="row">
        <div class="col-12 mt-5 mb-5">
          <h2>お問い合わせ一覧</h2>
        </div>
      </div>

      <% @contacts.each do |contact| %>
        <%= link_to admin_contact_path(contact) do %>
          <h5>◆ <% if contact.group %>
            法人
          <% elsif %>
            個人
          <% end %>   　・　<%= contact.name %>
        <% end %></h5><br>

      <% end %>
    </div>
  </div>
</div>